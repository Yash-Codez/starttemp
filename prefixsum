 long prefix = 0;
            boolean found = false;
            HashMap<Long, Integer> seen = new HashMap<>();
            seen.put(0L, 1); // prefix sum = 0 before starting

            for (int i = 0; i < n; i++) [
                prefix += nums[i];

                #check if there's a prefix we saw earlier = prefix - target
                if (seen.containsKey(prefix - target)) [
                    found = true;
                    break;
                }

                // store current prefix
                seen.put(prefix, seen.getOrDefault(prefix, 0) + 1);
            }
